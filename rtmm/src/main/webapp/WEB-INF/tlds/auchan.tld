			<description></description>
		</attribute>

		<attribute>
			<name>btntext</name>
			<rtexprvalue>true</rtexprvalue>
			<description></description>
		</attribute>

		<attribute>
			<name>btnautofocus</name>
			<rtexprvalue>true</rtexprvalue>
			<description>rule the button will get the focus automatically when
				uploading the page</description>
		</attribute>

		<attribute>
			<name>btnform</name>
			<rtexprvalue>true</rtexprvalue>
			<description>rule the button belongs to one or more forms </description>
		</attribute>
	</tag>
