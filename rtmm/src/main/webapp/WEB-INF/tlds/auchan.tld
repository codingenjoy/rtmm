	<tag>
		<name>menu</name>
		<tag-class>com.auchan.common.webtag.tree.MenuTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<name>id</name>
			<rtexprvalue>true</rtexprvalue>
			<description>this is the code of menu</description>
		</attribute>
		<attribute>
			<name>isFirstLoad</name>
			<rtexprvalue>true</rtexprvalue>
			<description>this is a bool value,if true,the menu data loaded in
				jspfirstly,and vice versa</description>
		</attribute>
		<attribute>
			<name>pid</name>
			<rtexprvalue>true</rtexprvalue>
			<description></description>
		</attribute>
	</tag>

	<tag>
		<name>button</name>
		<tag-class>com.auchan.common.webtag.oper.ButtonTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<name>modulekey</name>
			<rtexprvalue>true</rtexprvalue>
			<description></description>
		</attribute>
