			<rtexprvalue>true</rtexprvalue>
			<description></description>
		</attribute>

		<attribute>
			<name>ctrlkey</name>
			<rtexprvalue>true</rtexprvalue>
			<description>this is necessary attribute,if not,the button will be
				gray and not be used.</description>
		</attribute>

		<attribute>
			<name>btnclass</name>
			<rtexprvalue>true</rtexprvalue>
			<description></description>
		</attribute>

		<attribute>
			<name>btnevent</name>
			<rtexprvalue>true</rtexprvalue>
			<description></description>
		</attribute>

		<attribute>
			<name>btntype</name>
